<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE decorators>
<decorators defaultdir="/WEB-INF/jsp/decorators">
	<excludes>
		<pattern>/popup/*</pattern>
		<pattern>/attachments*</pattern>
		<pattern>/wikis/preview*</pattern>
	</excludes>
	
	<decorator name="boards" page="boards.jsp">
		<pattern>/boards*</pattern>
		<pattern>/blogs*</pattern>
	</decorator>

	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
</decorators>